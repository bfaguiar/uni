package exercicio1;

public class Localidade extends Regiao {
	
	//extender regiao
	private TipoLocalidade localidade;
	
	public Localidade(String nome, int populacao,TipoLocalidade localidade) {
		super(nome, populacao);
		this.localidade = localidade;
	}

	public TipoLocalidade getLocalidade() {
		return localidade;
	}

	public void setLocalidade(TipoLocalidade localidade) {
		this.localidade = localidade;
	}
	
	
	
